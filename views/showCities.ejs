<style>
    table {border: 2px solid black}
    th{ border: 2px solid black}
    td {border: 2px solid black}
</style>

<h1>Cities</h1>
<table>
    <tr>
        <th>City Code</th>
        <th>Country Code</th>
        <th>Cty Name</th>
        <th>Population</th>
        <th>By the sea</th>
        <th>Area</th>
        <% cities.forEach((city)=>{ %>
    <tr>

        <td>
            <%= city.cty_code%>
        </td>
        <td>
            <%= city.co_code%>
        </td>
        <td>
            <%= city.cty_name%>
        </td>
        <td>
            <%= city.population%>
        </td>
        <td>
            <%= city.isCoastal%>
        </td>
        <td>
            <%= city.areaKM%>
        </td>
        <td>
            <a href="/cities/allDetails/<%=city.cty_code%>">All Details</a>
        </td>
    </tr>
    <% }) %>
</table>
<a href="../">home</a>